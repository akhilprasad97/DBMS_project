
  <div class="imgcontainer">
    <h1 class="cyan-text center">Customs Management System</h1>
  </div>
  <div class="container">
    <div class="errBox red center">
    <h5 class="white-text" class="center errText">
      {{tagline}}
    </h5>
  </div>
  <form action={{link}} method="post">
    <label><b>Passneger Name</b></label>
    <input type="text" placeholder="Enter Passenger Name" name="pname" required>
    <label><b>Passport Number</b></label>
    <input type="text" placeholder="Enter Passport Number" name="pno" required>
    <label><b>Nationality</b></label>
    <input type="text" placeholder="Enter Nationality" name="nation" required>
    <label><b>Visa Number</b></label>
    <input type="text" placeholder="Enter Visa Number" name="visa" required>
    <button type="submit" class="btn waves-light waves-effect center">Enter Luggage details
      <i class="material-icons right">send</i>
    </button>
</form>
